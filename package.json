{
  "name": "pavk",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@slidev/cli": "^0.50.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "slidev-theme-academic": "^2.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.279",
    "playwright-chromium": "^1.49.0",
    "prettier": "^3.3.3",
    "prettier-plugin-slidev": "^1.0.5",
    "unocss-preset-tailwindcss-motion": "^4.0.1",
    "unplugin-icons": "^0.21.0"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "slides.md",
          "pages/*.md"
        ],
        "options": {
          "parser": "slidev",
          "plugins": [
            "prettier-plugin-slidev"
          ]
        }
      }
    ]
  }
}
